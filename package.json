{
  "name": "mongodb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "copy-graphql": "copyfiles -e \"**/*.ts\" -e \"**/*.map\" -e \"**/*.d.ts\" \"src/**/*.graphql\" lib",
    "build": "rm -rf lib && yarn tsc -w && yarn copy-graphql",
    "dev": "nodemon ./bin/www"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.19",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.6.0",
    "graphql-http": "^1.18.0",
    "graphql-tag": "^2.12.6",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.4",
    "morgan": "~1.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5",
    "@types/copyfiles": "^2",
    "@types/jsonwebtoken": "^9",
